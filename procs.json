{
    "web-server": {
        "command": "nodemon"
      , "color": "blue"
      , "env": {
            "NODE_ENV": "development"
          , "PORT": "3000"
        }
      , "cwd": "/apps/webirc"
      , "args": [
            "--debug ./app.js"
          , "--watch ./lib"
          , "--watch ./pub"
          , "--watch ./app.js"
        ]
    }
  , "irc-server": {
        "command": "node"
      , "color": "yellow"
      , "cwd": "/apps/ircd.js"
      , "args": [
            "/apps/ircd.js/bin/ircd.js"
        ]
    }
}
